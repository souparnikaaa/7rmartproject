<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="crossbrowsingtesting">

<listeners><listener class-name="report.Listeners">
</listener>
</listeners>
  <test  name="ChromeTest">
  <parameter name="browser" value="chrome"></parameter>
    <classes>
     
      <class name="testscripts.LoginTest">
      <methods>
      <include name="verifywhetheruserisabletologinwithcorrectusernameandpassword">
      </include>
      
      
      </methods>
      </class>
      
    </classes>
  </test>
  
  <test  name="FirefoxTest">
